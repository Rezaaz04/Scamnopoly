                        Player2Money.Text = "" + kauf;
                        besitzGrün3 = true;
                        MessageBox.Show("Spieler 2 besitzt jetzt eine neue Straße!");
                        spieler2Besitz.Add("StraßeGreen3.Visible");
                        lblP2Grün3.BackColor = Color.Green;
                    }
                    else
