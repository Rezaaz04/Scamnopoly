                {
                    MessageBox.Show("Sie müssen Miete zahlen!");
                    int Miete = Convert.ToInt32(Player2Money.Text) - 4 * Convert.ToInt32(showFeldNum.Text);
                    Player2Money.Text = "" + Miete;
                    int verdienst = Convert.ToInt32(Player1Money.Text) + 4 * Convert.ToInt32(showFeldNum.Text);
                    Player1Money.Text = "" + verdienst;
                }
                else if (spieler1Besitz.Contains("Wasserwerk") && Wasserwerk.Visible)
                {
