                        spieler1Besitz.Add("StraßeTürkis1");
                        lblP1Türkis1.BackColor = Color.PaleTurquoise;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
                else if (playerTurn.Text == "Spieler 2")
                {
                    if (Convert.ToInt32(Player2Money.Text) == 100 || besitzTürkis1 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player2Money.Text) - 100;
                        Player2Money.Text = "" + kauf;
                        besitzTürkis1 = true;
                        MessageBox.Show("Spieler 2 besitzt jetzt eine neue Straße!");
                        spieler2Besitz.Add("StraßeTürkis1");
                        lblP2Türkis1.BackColor = Color.PaleTurquoise;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
            }
            else if (StraßeTürkis2.Visible)
            {
                if (playerTurn.Text == "Spieler 1") //Prüft welcher Spieler auf der Straße ist.
                {
                    if (Convert.ToInt32(Player1Money.Text) == 100 || besitzTürkis2 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
