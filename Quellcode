        void Gehalt()
        {
            MessageBox.Show("Gehalt: 200$");

            if (playerTurn.Text == "Spieler 1")
            {
                int P1money = Convert.ToInt32(Player1Money.Text) + 200;
                Player1Money.Text = "" + P1money;
            }
            else if (playerTurn.Text == "Spieler 2")
            {
                int P2money = Convert.ToInt32(Player2Money.Text) + 200;
                Player2Money.Text = "" + P2money;
            }
        }

        Random eCard = new Random();
        Random gCard = new Random();
        void ShowEventcardE()
        {
            int ereignis = eCard.Next(1, 6);

            if(ereignis == 1)
            {
                Ereignisfeld1.Visible = true;
            }
            else if (ereignis == 2)
            {
                Ereignisfeld2.Visible = true;
            }
            else if (ereignis == 3)
            {
                Ereignisfeld3.Visible = true;
            }
            else if (ereignis == 4)
            {
                Ereignisfeld4.Visible = true;
            }
            else if (ereignis == 5)
            {
                Ereignisfeld5.Visible = true;
            }
            else if (ereignis == 6)
            {
                Ereignisfeld6.Visible = true;
            }
        }

        void HideEventcardE()
        {
            Ereignisfeld1.Visible = false;
            Ereignisfeld2.Visible = false;
            Ereignisfeld3.Visible = false;
            Ereignisfeld4.Visible = false;
            Ereignisfeld5.Visible = false;
            Ereignisfeld6.Visible = false;
        }

        void ShowEventcardG()
        {
            int gemeinschaft = gCard.Next(1, 6);

            if(gemeinschaft == 1)
            {
                Gemeinschaftfeld1.Visible = true;
            }
            else if (gemeinschaft == 2)
            {
                Gemeinschaftsfeld2.Visible = true;
            }
            else if (gemeinschaft == 3)
            {
                Gemeinschaftsfeld3.Visible = true;
            }
            else if (gemeinschaft == 4)
            {
                Gemeinschaftsfeld4.Visible = true;
            }
            else if (gemeinschaft == 5)
            {
                Gemeinschaftsfeld5.Visible = true;
            }
            else if (gemeinschaft == 6)
            {
                Gemeinschaftsfeld6.Visible = true;
            }
        }

        void HideEventcardG()
        {
            Gemeinschaftfeld1.Visible = false;
            Gemeinschaftsfeld2.Visible = false;
            Gemeinschaftsfeld3.Visible = false;
            Gemeinschaftsfeld4.Visible = false;
            Gemeinschaftsfeld5.Visible = false;
            Gemeinschaftsfeld6.Visible = false;
        }

        bool besitzBraun1 = false;
        bool besitzBraun2 = false;
        bool besitzTürkis1 = false;
        bool besitzTürkis2 = false;
        bool besitzTürkis3 = false;
        bool besitzPink1 = false;
        bool besitzPink2 = false;
        bool besitzPink3 = false;
        bool besitzOrange1 = false;
        bool besitzOrange2 = false;
        bool besitzOrange3 = false;
        bool besitzRot1 = false;
        bool besitzRot2 = false;
        bool besitzRot3 = false;
        bool besitzGelb1 = false;
        bool besitzGelb2 = false;
        bool besitzGelb3 = false;
        bool besitzGrün1 = false;
        bool besitzGrün2 = false;
        bool besitzGrün3 = false;
        bool besitzBlau1 = false;
        bool besitzBlau2 = false;
        bool besitzSüdbahnhof = false;
        bool besitzWestbahnhof = false;
        bool besitzNordbahnhof = false;
        bool besitzHauptbahnhof = false;
        bool besitzWasserwerk = false;
        bool besitzElekrizitätswerk = false;

        private void bttnBuy_Click(object sender, EventArgs e) //Der Spieler kann die Straße kaufen.
        {
            if(StraßeBraun1.Visible) //Prüft welche Straße sichtbar ist.
            {
                if(playerTurn.Text == "Spieler 1") //Prüft welcher Spieler auf der Straße ist.
                {
                    if (Convert.ToInt32(Player1Money.Text) == 60 || besitzBraun1 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player1Money.Text) - 60;
                        Player1Money.Text = "" + kauf;
                        besitzBraun1 = true;
                        MessageBox.Show("Spieler 1 besitzt jetzt eine neue Straße!");
                        spieler1Besitz.Add("StraßeBraun1");
                        lblP1Braun1.BackColor = Color.Brown;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
                else if(playerTurn.Text == "Spieler 2")
                {
                    if (Convert.ToInt32(Player2Money.Text) == 60 || besitzBraun1 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player2Money.Text) - 60;
                        Player2Money.Text = "" + kauf;
                        besitzBraun1 = true;
                        MessageBox.Show("Spieler 2 besitzt jetzt eine neue Straße!");
                        spieler2Besitz.Add("StraßeBraun1");
                        lblP2Braun1.BackColor = Color.Brown;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
            }
            else if(StraßeBraun2.Visible)
            {
                if (playerTurn.Text == "Spieler 1") //Prüft welcher Spieler auf der Straße ist.
                {
                    if (Convert.ToInt32(Player1Money.Text) == 60 || besitzBraun2 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player1Money.Text) - 60;
                        Player1Money.Text = "" + kauf;
                        besitzBraun1 = true;
                        MessageBox.Show("Spieler 1 besitzt jetzt eine neue Straße!");
                        spieler1Besitz.Add("StraßeBraun2");
                        lblP1Braun2.BackColor = Color.Brown;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
                else if (playerTurn.Text == "Spieler 2")
                {
                    if (Convert.ToInt32(Player2Money.Text) == 60 || besitzBraun2 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player2Money.Text) - 60;
                        Player2Money.Text = "" + kauf;
                        besitzBraun1 = true;
                        MessageBox.Show("Spieler 2 besitzt jetzt eine neue Straße!");
                        spieler2Besitz.Add("StraßeBraun2");
                        lblP2Braun2.BackColor = Color.Brown;
                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
            }
            else if(StraßeTürkis1.Visible)
            {
                if (playerTurn.Text == "Spieler 1") //Prüft welcher Spieler auf der Straße ist.
                {
                    if (Convert.ToInt32(Player1Money.Text) == 100 || besitzTürkis1 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player1Money.Text) - 100;
