        private void bttnMenu_Click(object sender, EventArgs e)
        {
            mainmenu.Show();
        }

        private void bttnBankrott_Click(object sender, EventArgs e) //Spieler geht Bankrott
        {
            if (playerTurn.Text == "Spieler 1")  //Prüft welcher Spieler dran ist.
            {
                MessageBox.Show("Wer auch immer Spieler 1 ist, also mir wäre diese Niederlage echt PEINLICH!");
                MessageBox.Show("Schäm dich!");
                Close();
            }
            else if (playerTurn.Text == "Spieler 2")
            {
                MessageBox.Show("Wer auch immer Spieler 2 ist, also mir wäre diese Niederlage echt PEINLICH!");
                MessageBox.Show("Schäm dich!");
                Close();
            }
        }
    }
