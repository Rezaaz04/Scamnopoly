        private void zugBeenden_Click(object sender, EventArgs e)   //Der Spieler muss sein Zug manuel beenden.
        {
            Hidestreetcard();
            zugBeenden.Visible = false;
            button1.Visible = true;

            if (playerTurn.Text == "Spieler 1")  //Prüft welcher Spieler dran ist.
            {
                playerTurn.Text = "Spieler 2";
                label3.Text = "2";
            }
            else if (playerTurn.Text == "Spieler 2")
            {
                playerTurn.Text = "Spieler 1";
                label3.Text = "1";
            }
        }

        Random würfeln1 = new Random(); //Würfel von Spieler 1
        Random würfeln2 = new Random(); //Würfel von Spieler 2

        int würfelNum1(int w1)
        {
            return w1;
        }
        int würfelNum2(int w2)
        {
            return w2;
        }

        private void button1_Click(object sender, EventArgs e)  //Eine zufällige Zahl wird gewürfelt.
        {
            int feldNummerP1;
            int feldNummerP2;
            Hidestreetcard();
            HideEventcardE();
            HideEventcardG();

            if (playerTurn.Text == "Spieler 1")  //Prüft welcher Spieler dran ist.
            {
                int playerWürfelt1 = würfeln1.Next(2, 12);
                int numbercounter1 = Convert.ToInt32(player1position.Text);
                feldNummerP1 = numbercounter1 + playerWürfelt1;
                würfelNum1(playerWürfelt1);
                showFeldNum.Text = "" + playerWürfelt1;

                player1position.Text = Convert.ToString(feldNummerP1);
                Player1moving();                
            }
            else if (playerTurn.Text == "Spieler 2")
            {
                int playerWürfelt2 = würfeln1.Next(2, 12);
                int numbercounter2 = Convert.ToInt32(player2position.Text);
                feldNummerP2 = numbercounter2 + playerWürfelt2;
                würfelNum1(playerWürfelt2);
                showFeldNum.Text = "" + playerWürfelt2;

                player2position.Text = Convert.ToString(feldNummerP2);                
                Player2moving();                
            }
            PlayerPaysRent();
            button1.Visible = false;
            zugBeenden.Visible = true;
        }

