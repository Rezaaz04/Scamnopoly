        void PlayerPaysRent() //Der Spieler der aud das Grundstück des anderen Spielers betritt, muss Miete Zahlen.
        {
            if (playerTurn.Text == "Spieler 1")
            {
                if (spieler1Besitz.Contains("StraßeBraun1") && StraßeBraun1.Visible)
                {
                    MessageBox.Show("Sie müssen Miete zahlen!");
                    int Miete = Convert.ToInt32(Player2Money.Text) - 2;
                    Player2Money.Text = "" + Miete;
                    int verdienst = Convert.ToInt32(Player1Money.Text) + 2;
                    Player1Money.Text = "" + verdienst;
                }
                else if (spieler1Besitz.Contains("StraßeBraun2") && StraßeBraun2.Visible)
                {
                    MessageBox.Show("Sie müssen Miete zahlen!");
                    int Miete = Convert.ToInt32(Player2Money.Text) - 2;
                    Player2Money.Text = "" + Miete;
                    int verdienst = Convert.ToInt32(Player1Money.Text) + 2;
                    Player1Money.Text = "" + verdienst;
                }
                else if (spieler1Besitz.Contains("StraßeTürkis1") && StraßeTürkis1.Visible)
                {
                    MessageBox.Show("Sie müssen Miete zahlen!");
                    int Miete = Convert.ToInt32(Player2Money.Text) - 6;
                    Player2Money.Text = "" + Miete;
                    int verdienst = Convert.ToInt32(Player1Money.Text) + 6;
                    Player1Money.Text = "" + verdienst;
                }
                else if (spieler1Besitz.Contains("StraßeTürkis2") && StraßeTürkis2.Visible)
                {
                    MessageBox.Show("Sie müssen Miete zahlen!");
