                    }
                    else
                    {
                        MessageBox.Show("Du hast entweder nicht genung Geld, oder dieses Gebäude besitzt schon jemand!");
                    }
                }
                else if (playerTurn.Text == "Spieler 2")
                {
                    if (Convert.ToInt32(Player2Money.Text) == 100 || besitzTürkis2 == false) //Prüft ob der Spieler das Geld hat und ob die Straße verfügbar ist.
                    {
                        int kauf = Convert.ToInt32(Player2Money.Text) - 100;
                        Player2Money.Text = "" + kauf;
                        besitzTürkis2 = true;
