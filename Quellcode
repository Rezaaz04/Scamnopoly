        MainMenu mainmenu = new MainMenu();

        private void changeSkin1_Click(object sender, EventArgs e)  //Der Erste Spieler darf sein Skin wählen.
        {
            string i = skinChangeNum1.Text;
            skinChangeNum1.Text = "0";

            if (i == "0")
            {
                skinAmogus1.Visible = true;
                skinChangeNum1.Text = "1";
            }
            else if (i == "1")
            {
                skinAuto1.Visible = true;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "2";
            }
            else if (i == "2")
            {
                skinEiffel1.Visible = true;
                skinAuto1.Visible = false;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "3";
            }
            else if (i == "3")
            {
                skinPlane1.Visible = true;
                skinEiffel1.Visible = false;
                skinAuto1.Visible = false;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "4";
            }
            else if (i == "4")
            {
                skinBurger1.Visible = true;
                skinPlane1.Visible = false;
                skinEiffel1.Visible = false;
                skinAuto1.Visible = false;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "5";
            }
            else if (i == "5")
            {
                skinSkateboard1.Visible = true;
                skinBurger1.Visible = false;
                skinPlane1.Visible = false;
                skinEiffel1.Visible = false;
                skinAuto1.Visible = false;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "0";
            }
            else
            {
                skinSkateboard1.Visible = false;
                skinBurger1.Visible = false;
                skinPlane1.Visible = false;
                skinEiffel1.Visible = false;
                skinAuto1.Visible = false;
                skinAmogus1.Visible = false;
                skinChangeNum1.Text = "0";
            }
        }

        private void changeSkin2_Click(object sender, EventArgs e) //Der Zweite Spieler darf sein Skin wählen.
