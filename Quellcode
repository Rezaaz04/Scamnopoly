                autoPlayer2.Location = new Point(1396, 622);
                skateboardPlayer2.Location = new Point(1396, 622);
            }
            else if (Convert.ToInt32(player2position.Text) == 38)
            {
                amogusPlayer2.Location = new Point(1396, 699);
                planePlayer2.Location = new Point(1396, 699);
                eiffelPlayer2.Location = new Point(1396, 699);
                burgerPlayer2.Location = new Point(1396, 699);
                autoPlayer2.Location = new Point(1396, 699);
                skateboardPlayer2.Location = new Point(1396, 699);
            }
            else if (Convert.ToInt32(player2position.Text) == 39)
            {
                amogusPlayer2.Location = new Point(1396, 787);
                planePlayer2.Location = new Point(1396, 787);
                eiffelPlayer2.Location = new Point(1396, 787);
                burgerPlayer2.Location = new Point(1396, 787);
                autoPlayer2.Location = new Point(1396, 787);
                skateboardPlayer2.Location = new Point(1396, 787);
            }

            PlayEventOnField();
        }

        private void bttnStart_Click(object sender, EventArgs e) //Startet das Spiel.
        {
            Hidestreetcard();
            button1.Visible = true;
            bttnStart.Visible = false;
            changeSkin1.Visible = false;
            changeSkin2.Visible = false;

            Checkskin1();
            Checkskin2();
        }

        void Checkskin1() //Überprüft welcher Skin von Spieler 1 verwendet wird.
        {
            if (skinAmogus1.Visible)
            {
                amogusPlayer1.Visible = true;
            }
            else if (skinAuto1.Visible)
            {
                autoPlayer1.Visible = true;
            }
            else if (skinBurger1.Visible)
            {
                burgerPlayer1.Visible = true;
            }
            else if (skinEiffel1.Visible)
            {
                eiffelPlayer1.Visible = true;
            }
            else if (skinPlane1.Visible)
            {
                planePlayer1.Visible = true;
            }
            else if (skinSkateboard1.Visible)
            {
                skateboardPlayer1.Visible = true;
            }
        }

        void Checkskin2() //Überprüft welcher Skin von Spieler 2 verwendet wird.
        {
            if (skinAmogus2.Visible)
            {
                amogusPlayer2.Visible = true;
            }
            else if (skinAuto2.Visible)
            {
                autoPlayer2.Visible = true;
            }
            else if (skinBurger2.Visible)
            {
                burgerPlayer2.Visible = true;
            }
            else if (skinEiffel2.Visible)
            {
                eiffelPlayer2.Visible = true;
            }
            else if (skinPlane2.Visible)
            {
                planePlayer2.Visible = true;
            }
            else if (skinSkateboard2.Visible)
            {
                skateboardPlayer2.Visible = true;
            }
        }

        void Hidestreetcard()  //Macht alle Karten unsichtbar.
        {
            StraßeBraun1.Visible = false;
            StraßeBraun2.Visible = false;
            StraßeGelb1.Visible = false;
            StraßeGelb2.Visible = false;
            StraßeGelb3.Visible = false;
            StraßeRot1.Visible = false;
            StraßeRot2.Visible = false;
            StraßeRot3.Visible = false;
            StraßeTürkis1.Visible = false;
            StraßeTürkis2.Visible = false;
            StraßeTürkis3.Visible = false;
            StraßePink1.Visible = false;
            StraßePink2.Visible = false;
            StraßePink3.Visible = false;
            Südbahnhof.Visible = false;
            Hauptbahnhof.Visible = false;
            Westbahnhof.Visible = false;
            Nordbahnhof.Visible = false;
            StraßeOrange1.Visible = false;
            StraßeOrange2.Visible = false;
            StraßeOrange3.Visible = false;
            StraßeBlau1.Visible = false;
            StraßeBlau2.Visible = false;
            StraßeGreen1.Visible = false;
            StraßeGreen2.Visible = false;
            StraßeGreen3.Visible = false;
            Wasserwerk.Visible = false;
            Elektrizitätswerk.Visible = false;
        }

        void PlayEventOnField() //Der Spieler löst auf das gelandete Feld ein Event aus.
        {
            if (playerTurn.Text == "Spieler 2") //Überprüft zuerst welcher Spieler dran ist.
            {
                if (Convert.ToInt32(player2position.Text) == 1) //Überprüft jetzt wo sich der Spieler befindet.
                {
                    StraßeBraun1.Visible = true;
                }
                else if (Convert.ToInt32(player2position.Text) == 2)
                {
                    ShowEventcardE();
                }
                else if (Convert.ToInt32(player2position.Text) == 3)
                {
